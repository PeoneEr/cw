<h1>Информация о складах</h1>
<table>
  <thead>
    <th>Название</th>
    <th>Телефон</th>
    <th>Адрес</th>
    <th>Дата принятия</th>
    <th></th>
    <th></th>
  </thead>
  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.title %></td>
        <td><%= store.phone %></td>
        <td><%= store.address %></td>
        <td><%= store.created_at %></td>
        <td><%= link_to "Изменить", edit_store_path(store.id) %></td>
        <td><%= link_to "Удалить", store_path(store.id), method: :delete, confirm: "Вы уверены?" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<% if params[:utf8] %>
  <h1>Информация о поставках</h1>
  <%= render 'supplies/table' %>
<% end %>
